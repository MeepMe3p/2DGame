<AnchorPane prefHeight="328.0" prefWidth="429.0" style="-fx-background-color: transparent;" stylesheets="@anchorpane_style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx_plus_willpower.controller.SignUpController">
   <children>
      <Pane prefHeight="317.0" prefWidth="429.0">
         <children>
            <ImageView fitHeight="295.0" fitWidth="485.0" layoutX="-13.0" layoutY="46.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../assets/Background%20Register.jpg" />
               </image>
            </ImageView>
            <TextField fx:id="textField_UserName" alignment="CENTER" layoutX="108.0" layoutY="98.0" promptText="User Name" stylesheets="@inputs_style.css" />
            <PasswordField fx:id="passwordField_Password" alignment="CENTER" layoutX="108.0" layoutY="140.0" promptText="Password" stylesheets="@inputs_style.css" />
            <PasswordField fx:id="passwordField_PasswordRepeat" alignment="CENTER" layoutX="108.0" layoutY="182.0" promptText="Repeat Password" stylesheets="@inputs_style.css" />
            <Button fx:id="btn_RegisterButton" contentDisplay="CENTER" layoutX="138.0" layoutY="225.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="152.0" stylesheets="@button_style.css" text="Submit" textFill="WHITE" />
